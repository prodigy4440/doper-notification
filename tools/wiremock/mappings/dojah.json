{
  "mappings": [
    {
      "request": {
        "method": "POST",
        "url": "/api/v1/dojah/send/sms",
        "bodyPatterns": [
          {
            "matchesJsonPath": "$.phone_number",
            "matches": "^(?i)200.*"
          }
        ]
      },
      "response": {
        "status": 200,
        "bodyFileName": "200_hollatag.json",
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "POST",
        "url": "/api/v1/dojah/send/sms",
        "bodyPatterns": [
          {
            "matchesJsonPath": "$.phone_number",
